{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["> 0.5%", "last 2 versions", "Firefox ESR"]
        }
      }
    ],
    "react"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": false
      }
    ],
    ["transform-class-properties", { "spec": true }],
    "transform-object-rest-spread"
  ]
}
