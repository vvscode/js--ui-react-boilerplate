{
  "name": "ui-react-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "React Starter",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "jest --coverage",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "generate": "node node_modules/yo/lib/cli.js ./generators/run.js",
    "generate-debug": "node --inspect-brk node_modules/yo/lib/cli.js ./generators/run.js"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "**/*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>src/setupTests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "statements": 80
      }
    },
    "moduleNameMapper": {
      "^@/(.*)": "<rootDir>/src/$1.js"
    }
  },
  "dependencies": {
    "core-js": "^2.5.3",
    "material-ui": "^1.0.0-beta.34",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-form": "^2.16.3",
    "react-input-mask": "^1.2.2",
    "react-jss": "^8.3.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0",
    "yeoman-generator": "^2.0.3"
  },
  "devDependencies": {
    "@exeto/eslint-config": "^1.0.0",
    "@exeto/eslint-config-react": "^1.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.3.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "lint-staged": "^6.1.1",
    "prettier": "^1.10.2",
    "prop-types": "^15.6.0",
    "react-dev-utils": "^5.0.0",
    "react-hot-loader": "^3.1.3",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "yo": "^2.0.1"
  }
}
